{
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
  "ecmaVersion": 2021,
  "sourceType": "module"
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-unused-vars": "error",
    "no-console": "off",
    "no-undef": "off",
    "semi": ["error", "always"],
    "eqeqeq": "error"
  }
}