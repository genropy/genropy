<?xml version="1.0" encoding="UTF-8"?>
<GenRoBag>
	<packages tag="package_list">
		<trac sqlprefix="" tag="package">
			<tables tag="table_list">
				<attachment tag="table">
					<columns tag="column_list">
						<type dtype="T" tag="column" notnull="y"/>
						<id dtype="T" tag="column" notnull="y"/>
						<filename dtype="T" tag="column" notnull="y"/>
						<size dtype="I" tag="column"/>
						<time dtype="I" tag="column"/>
						<description dtype="T" tag="column"/>
						<author dtype="T" tag="column"/>
						<ipnr dtype="T" tag="column"/>
					</columns>
				</attachment>
				<auth_cookie tag="table">
					<columns tag="column_list">
						<cookie dtype="T" tag="column" notnull="y"/>
						<name dtype="T" tag="column" notnull="y"/>
						<ipnr dtype="T" tag="column" notnull="y"/>
						<time dtype="I" tag="column"/>
					</columns>
				</auth_cookie>
				<component pkey="name" tag="table">
					<columns tag="column_list">
						<name dtype="T" tag="column" notnull="y"/>
						<owner dtype="T" tag="column"/>
						<description dtype="T" tag="column"/>
					</columns>
				</component>
				<enum tag="table">
					<columns tag="column_list">
						<type dtype="T" tag="column" notnull="y"/>
						<name dtype="T" tag="column" notnull="y"/>
						<value dtype="T" tag="column"/>
					</columns>
				</enum>
				<milestone pkey="name" tag="table">
					<columns tag="column_list">
						<name dtype="T" tag="column" notnull="y"/>
						<due dtype="I" tag="column"/>
						<completed dtype="I" tag="column"/>
						<description dtype="T" tag="column"/>
					</columns>
				</milestone>
				<node_change tag="table">
					<columns tag="column_list">
						<rev dtype="T" tag="column" notnull="y"/>
						<path dtype="T" tag="column" notnull="y"/>
						<node_type dtype="T" tag="column"/>
						<change_type dtype="T" tag="column" notnull="y"/>
						<base_path dtype="T" tag="column"/>
						<base_rev dtype="T" tag="column"/>
					</columns>
				</node_change>
				<permission tag="table">
					<columns tag="column_list">
						<username dtype="T" tag="column" notnull="y"/>
						<action dtype="T" tag="column" notnull="y"/>
					</columns>
				</permission>
				<report pkey="id" tag="table">
					<columns tag="column_list">
						<id  dtype="I" tag="column" notnull="y"/>
						<author dtype="T" tag="column"/>
						<title dtype="T" tag="column"/>
						<query dtype="T" tag="column"/>
						<description dtype="T" tag="column"/>
					</columns>
				</report>
				<revision pkey="rev" tag="table">
					<columns tag="column_list">
						<rev dtype="T" tag="column" notnull="y"/>
						<time dtype="I" tag="column"/>
						<author dtype="T" tag="column"/>
						<message dtype="T" tag="column"/>
					</columns>
				</revision>
				<session tag="table">
					<columns tag="column_list">
						<sid dtype="T" tag="column" notnull="y"/>
						<authenticated dtype="I" tag="column" notnull="y"/>
						<last_visit dtype="I" tag="column"/>
					</columns>
				</session>
				<session_attribute tag="table">
					<columns tag="column_list">
						<sid dtype="T" tag="column" notnull="y"/>
						<authenticated dtype="I" tag="column" notnull="y"/>
						<name dtype="T" tag="column" notnull="y"/>
						<value dtype="T" tag="column"/>
					</columns>
				</session_attribute>
				<system pkey="name" tag="table">
					<columns tag="column_list">
						<name dtype="T" tag="column" notnull="y"/>
						<value dtype="T" tag="column"/>
					</columns>
				</system>
				<ticket pkey="id" tag="table">
					<columns tag="column_list">
						<id dtype="I" tag="column" notnull="y"/>
						<type dtype="T" tag="column"/>
						<time dtype="I" tag="column"/>
						<changetime dtype="I" tag="column"/>
						<component dtype="T" tag="column"/>
						<severity dtype="T" tag="column"/>
						<priority dtype="T" tag="column"/>
						<owner dtype="T" tag="column"/>
						<reporter dtype="T" tag="column">
							<relation _T="NN" related_column="trac.permission.username"/>
						</reporter>
						<cc dtype="T" tag="column"/>
						<version dtype="T" tag="column"/>
						<milestone dtype="T" tag="column"/>
						<status dtype="T" tag="column"/>
						<resolution dtype="T" tag="column"/>
						<summary dtype="T" tag="column"/>
						<description dtype="T" tag="column"/>
						<keywords dtype="T" tag="column"/>
					</columns>
				</ticket>
				<ticket_change tag="table">
					<columns tag="column_list">
						<ticket dtype="I" tag="column" notnull="y">
							<relation _T="NN" related_column="trac.ticket.id"  one_name='changes'  many_name='ticket'/>
						</ticket>
						<time dtype="I" tag="column" notnull="y"/>
						<author dtype="T" tag="column"/>
						<field dtype="T" tag="column" notnull="y"/>
						<oldvalue dtype="T" tag="column"/>
						<newvalue dtype="T" tag="column"/>
					</columns>
				</ticket_change>
				<ticket_custom tag="table">
					<columns tag="column_list">
						<ticket dtype="I" tag="column" notnull="y"/>
						<name dtype="T" tag="column" notnull="y"/>
						<value dtype="T" tag="column"/>
					</columns>
				</ticket_custom>
				<version pkey="name" tag="table">
					<columns tag="column_list">
						<name dtype="T" tag="column" notnull="y"/>
						<time dtype="I" tag="column"/>
						<description dtype="T" tag="column"/>
					</columns>
				</version>
				<wiki tag="table">
					<columns tag="column_list">
						<name dtype="T" tag="column" notnull="y"/>
						<version dtype="I" tag="column" notnull="y"/>
						<time dtype="I" tag="column"/>
						<author dtype="T" tag="column"/>
						<ipnr dtype="T" tag="column"/>
						<text dtype="T" tag="column"/>
						<comment dtype="T" tag="column"/>
						<readonly dtype="I" tag="column"/>
					</columns>
				</wiki>
			</tables>
		</trac>
	</packages>
</GenRoBag>
